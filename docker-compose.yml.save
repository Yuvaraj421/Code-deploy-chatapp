version: '3'
services: 
  db: 
    image: postgres
    ports: 
      - 5432
    environment:
      - POSTGRES_USER=test
      - POSTGRES_PASSWORD=test
      - POSTGRES_DB=test_db
       
  app:
    build: .
    ports:
      - "8000:8000"
    expose:
      - 8000
    links:

      - db
    depends_on:
      - db

    
